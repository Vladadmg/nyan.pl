<script>


  


			$("#slider").easySlider({
				auto: true,
				continuous: true,
				firstShow: false,
				lastShow: false,
				prevText: '',
				nextText: '',
			});
		
		//$(".stop").onselectstart = function() { return false; };
		//$("img").onselectstart = function() { return false; };
		//$("img").attr('unselectable', 'on');
	
		$(".stop").on("click",function(e) {
			
			id=$(this).attr('id').substr(8);
			
			if ($("#obrazek_"+id).hasClass('stop')) {
					
					e.stopPropagation();
					e.stopImmediatePropagation();
					e.preventDefault();
					
					
					

					$("#obrazek_"+id).removeClass('stop').removeClass('stop16').removeClass('stop2').removeClass('stop4').removeClass('stop8').removeClass('stopsp').blur();
					$("#obrazek_"+id).hide();
					$("#obr_"+id).css('visibility','visible').hide();
					$("#obr_"+id).show();
					$("#obrazek_"+id).show();
					
					  if (document.selection)
						{
							document.selection.empty();
							obj.blur();
						}
						else
						{
							window.getSelection().removeAllRanges();
						}
					
					return false;
			}
		});
		
				user_id='0';
				
		$('.ulubione').click(function (e) {
			
			if (user_id==0) alert('By dodać obrazek do ulubionych - musisz być zalogowany.');
			
			id=$(this).attr('id');
			id=id.substr(7);
			url='include/ajax.php?wyk=zu&idu='+user_id+'&id='+id;
			//alert(url);
			$.get(url, function (data) {
				//alert(id);
				if (data=='OK') $('#pprzez_'+id).removeClass('pprzez');
					else $('#pprzez_'+id).addClass('pprzez');
				
			} );

		});	
		
		$('.ulubione').each(function (index,objekt) {
			if (user_id==0) return;
			
			id=$(this).attr('id');
			id=id.substr(7);
			$(this).removeClass('ukryj');
			//alert(id);
			url='include/ajax.php?wyk=su&idu='+user_id+'&id='+id;
			//alert(url);
			$.get(url, function (data) {
				//alert(id);
				if (data>0) {
					$('#pprzez_'+data).removeClass('pprzez');
					$('#pprzez_'+data).removeClass('ukryj');
				}
				else
				{				
					$('#pprzez_'+id.substr(1)).addClass('pprzez');
					$('#pprzez_'+id.substr(1)).addClass('ukryj');
				}
				
			});
			
		});
			

</script>
