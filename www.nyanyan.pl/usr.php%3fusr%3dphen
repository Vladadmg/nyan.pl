	function pokaz(id) {
		
		if ($("#obrazek_"+id).hasClass('stop')) {
			
			$("#obrazek_"+id).removeClass('stop').removeClass('stop16').removeClass('stop2').removeClass('stop4').removeClass('stop8').removeClass('stopsp').blur();
			
			$("#obrazek_"+id).hide();
			$("#obr_"+id).css('visibility','visible').hide();
			$("#obr_"+id).show();
			$("#obrazek_"+id).show();
			
			//alert(id);
			return false;
			
		}
		else
		{
			return true;
		}
	}
	
	
